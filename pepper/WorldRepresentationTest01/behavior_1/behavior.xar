                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.wr = ALProxy("ALWorldRepresentation")

    def onLoad(self):
        #put initialization code here
        self.names = []
        self.nameNowIdx = 0

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        CategoryName = self.getParameter("CategoryName")
        self.names = self.wr.getObjectsInCategory(CategoryName)
        print(self.names)
        self.nameNowIdx = 0
        self.onInput_onNext()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def onInput_onNext(self):
        if self.nameNowIdx >= len(self.names):
            self.nameNowIdx = 0
            self.names = []
            self.onInput_onStop()
        else:
            self.sendNext(self.names[self.nameNowIdx])
            self.nameNowIdx = self.nameNowIdx + 1]]>
